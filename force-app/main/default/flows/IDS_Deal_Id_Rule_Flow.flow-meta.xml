        <rules>
            <name>Yes_Deal_Id_Not_null</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>OrderFound.Deal_ID__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
