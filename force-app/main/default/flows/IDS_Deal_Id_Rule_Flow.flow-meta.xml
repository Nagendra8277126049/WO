        <name>Existing_Project_By_Deal_Id</name>
        <label>Existing Project By Deal Id</label>
        <locationX>52</locationX>
        <locationY>631</locationY>
        <defaultConnector>
            <targetReference>Set_output_to_no_association</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No Project found</defaultConnectorLabel>
        <rules>
            <name>Yes_Existing_Project</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Project</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Project.pse__Stage__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Closure</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Project.pse__Stage__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Cancelled</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_output_to_association</targetReference>
            </connector>
            <label>Yes Existing Project</label>
        </rules>
    </decisions>
