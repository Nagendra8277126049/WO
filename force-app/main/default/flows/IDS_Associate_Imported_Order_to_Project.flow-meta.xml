    <decisions>
        <name>Federal_Order_Validation</name>
        <label>Federal Order Validation</label>
        <locationX>44</locationX>
        <locationY>306</locationY>
        <defaultConnector>
            <targetReference>Invalid_federal_classification_To_Owner</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>Has_same_type</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>ImportedOrderIsFederal</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>ProjectToAssociateIsFederal</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_Sibling_Orders</targetReference>
            </connector>
            <label>Has same type</label>
        </rules>
    </decisions>
    <decisions>
        <name>If_Sibling_has_Same_Currency</name>
        <label>If Sibling has Same Currency</label>
        <locationX>646</locationX>
        <locationY>205</locationY>
        <defaultConnector>
            <targetReference>For_All_Siblings</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>Same_Currency</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Sibling.CurrencyIsoCode</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>OrderCurrency</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Associate_Order_to_Project</targetReference>
            </connector>
            <label>Same Currency</label>
        </rules>
    </decisions>
    <decisions>
        <name>Validate_Siblings</name>
