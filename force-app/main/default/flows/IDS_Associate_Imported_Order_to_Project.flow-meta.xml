        <defaultConnector>
            <targetReference>Associate_Order_to_Project</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>Has_Siblings</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Siblings</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>For_All_Siblings</targetReference>
            </connector>
            <label>Has Siblings</label>
        </rules>
    </decisions>
    <description>Added federal order validation</description>
    <formulas>
        <name>ChatterMessage</name>
        <dataType>String</dataType>
        <expression>{!Notify_Owner} &amp; &quot;The order &quot; &amp;  {!ImportedOrder.OrderNumber} &amp; &quot; was manually imported but its currency does not match the currency of the project. It has been sent to the Unnassociated Queue.&quot;</expression>
    </formulas>
    <formulas>
        <name>Error_Federal_Classification</name>
        <dataType>String</dataType>
        <expression>{!Notify_Owner} &amp; &quot; The order &quot;&amp;{!ImportedOrder.OrderNumber} &amp;&quot; was manually imported but does not match to the federal classification of the project. The order is available on the Unassociated list.&quot;</expression>
    </formulas>
    <formulas>
        <name>ImportedOrderIsFederal</name>
        <dataType>Boolean</dataType>
        <expression>CONTAINS(UPPER({!ImportedOrder.OCI_Order_Type__c}), &quot;FED&quot;) || CONTAINS(UPPER({!ImportedOrder.Sales_Channel__c}), &quot;FED&quot;)</expression>
    </formulas>
    <formulas>
        <name>Notify_Owner</name>
        <dataType>String</dataType>
        <expression>IF(ISBLANK({!UserId}),NULL, &quot;@[&quot; &amp; {!UserId} &amp; &quot;] &quot;)</expression>
    </formulas>
    <formulas>
        <name>ProjectToAssociateIsFederal</name>
        <dataType>Boolean</dataType>
        <expression>ISPICKVAL({!ProjectToAssociate.Project_Visibility__c}, &quot;Federal&quot;)</expression>
    </formulas>
    <interviewLabel>IDS - Associate Imported Order to Project {!$Flow.CurrentDateTime}</interviewLabel>
    <label>IDS - Associate Imported Order to Project</label>
