        <locationX>300</locationX>
        <locationY>179</locationY>
        <defaultConnector>
            <targetReference>Lookup_on_TimeZone_MDT</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>Is_Pending_Review</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>PendingReviewStatus</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Check_Current_Status</targetReference>
            </connector>
            <label>Is Pending Review</label>
        </rules>
    </decisions>
    <formulas>
        <name>EndDate_var</name>
        <dataType>DateTime</dataType>
        <expression>DATETIMEVALUE({!Form_ScheduleEndDat_var})-({!hoursOffsetValue_var}/24)-({!Minutes_Offset_var}/1440)</expression>
    </formulas>
    <formulas>
        <name>Form_Schedue_StartDate</name>
        <dataType>DateTime</dataType>
        <expression>DATETIMEVALUE(SUBSTITUTE( {!ScheduleStartDate},&#39;T&#39;,&#39; &#39;))</expression>
    </formulas>
    <formulas>
        <name>Form_ScheduleEndDat_var</name>
        <dataType>DateTime</dataType>
        <expression>DATETIMEVALUE(SUBSTITUTE( {!ScheduleEndDate},&#39;T&#39;,&#39; &#39;))</expression>
    </formulas>
    <formulas>
        <name>hoursOffsetValue_var</name>
        <dataType>Number</dataType>
        <expression>VALUE(LEFT({!offsetValue_var_frmul}, FIND(&quot;:&quot;,{!offsetValue_var_frmul})-1) )+1</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>isScheduleFlag_var</name>
        <dataType>Boolean</dataType>
        <expression>IF(NOT(ISNULL({!ScheduleEndDate})),true,false)</expression>
    </formulas>
    <formulas>
        <name>Minutes_Offset_var</name>
        <dataType>Number</dataType>
        <expression>VALUE(RIGHT(
{!offsetValue_var_frmul},
(LEN({!offsetValue_var_frmul})-FIND(&quot;:&quot;, {!offsetValue_var_frmul}))))</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <description>offset</description>
        <name>offsetValue_var_frmul</name>
        <dataType>String</dataType>
        <expression>MID({!Delta_TimeZone_var}, 5, 6)</expression>
    </formulas>
    <formulas>
        <name>scheduling_Internal_Logic_var</name>
        <dataType>String</dataType>
        <expression>IF(NOT(ISNULL( {!ScheduleStartDate})),&#39;Scheduled&#39;,&#39;Neither&#39;)</expression>
    </formulas>
    <formulas>
        <name>setWOEpitomeRefNumber</name>
        <dataType>String</dataType>
        <expression>IF(NOT(ISNULL({!Epitome_FSD_Ref})),{!Epitome_FSD_Ref}, {!woEpitomeRefNum})</expression>
    </formulas>
    <formulas>
        <description>StartDate</description>
        <name>StartDate_var</name>
        <dataType>DateTime</dataType>
        <expression>DATETIMEVALUE( {!Form_Schedue_StartDate})-({!hoursOffsetValue_var}/24)-({!Minutes_Offset_var}/1440)</expression>
    </formulas>
