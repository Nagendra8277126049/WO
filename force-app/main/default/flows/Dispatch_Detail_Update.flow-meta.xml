        </assignmentItems>
        <connector>
            <targetReference>Lookup_on_TimeZone_MDT</targetReference>
        </connector>
    </assignments>
    <constants>
        <name>PendingReviewStatus</name>
        <dataType>String</dataType>
        <value>
            <stringValue>Pending Review</stringValue>
        </value>
    </constants>
    <decisions>
        <name>Check_Current_Status</name>
        <label>Check Current Status</label>
        <locationX>303</locationX>
        <locationY>297</locationY>
        <defaultConnector>
            <targetReference>Assign_Current_Status</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>Is_Current_Status</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>currentWOStatus</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Open</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>currentWOStatus</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Pending Schedule</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>currentWOStatus</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Problem</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Lookup_on_TimeZone_MDT</targetReference>
            </connector>
            <label>Is Current Status</label>
        </rules>
    </decisions>
    <decisions>
        <name>Check_for_Work_Order_id</name>
